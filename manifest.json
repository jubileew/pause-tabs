{
    "name": "Mute Tabs",
    "description": "Mute all tabs other than current tab",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "scripting", "tabs"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [ {
        "all_frames": true,
        "js": [ "content.js" ],
        "matches": [ "<all_urls>" ]
     } ]
  }



